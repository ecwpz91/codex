# .bash_complete

# ----------------------------------------------------------------------
# OpenShift CLI Client
# ----------------------------------------------------------------------
if [ -f $HOME/.local/bin/oc.sh ]; then
 source $HOME/.local/bin/oc.sh
fi

# ----------------------------------------------------------------------
# OpenShift CLI Script
# ----------------------------------------------------------------------
if [ -f $HOME/.local/bin/oc-cluster.sh ]; then
 source $HOME/.local/bin/oc-cluster.sh
fi

# ----------------------------------------------------------------------
# Minikube
# ----------------------------------------------------------------------
if [ -f $HOME/.local/bin/minikube.sh ]; then
 source $HOME/.local/bin/minikube.sh
fi
