# .bash_export

# ----------------------------------------------------------------------
# Systemctl
# ----------------------------------------------------------------------
# export SYSTEMD_PAGER=

# ----------------------------------------------------------------------
# POSIX
# ----------------------------------------------------------------------
export PS1="\[\033[0;31m\]\t \[\033[m\]\\$ "
export PS2='continue-> '
export PS4='$0.$LINENO+ '
export CLICOLOR='1'
export LSCOLORS='ExFxBxDxCxegedabagacad'

# ----------------------------------------------------------------------
# Devel
# ----------------------------------------------------------------------
# export WORKSPACE="$HOME/workspace"

# ----------------------------------------------------------------------
# Brew
# ----------------------------------------------------------------------
# export HOMEBREW_CASK_OPTS=

# ----------------------------------------------------------------------
# Emacs
# ----------------------------------------------------------------------
# export EDITOR='emacs -nw'
# export VISUAL='emacs -nw'

# ----------------------------------------------------------------------
# Node.js
# ----------------------------------------------------------------------
# export NODE_NO_READLINE='1'
# export NPM_CONFIG_PREFIX=
# export PATH="$NPM_CONFIG_PREFIX/bin:$PATH"
# export N_PREFIX="$HOME/.n"; [[ :$PATH: == *":$N_PREFIX/bin:"* ]] || PATH+=":$N_PREFIX/bin"

# ----------------------------------------------------------------------
# Git
# ----------------------------------------------------------------------
export GITHUB_USER='ecwpz91'
export GIT_COMMITTER_NAME="${GITHUB_USER}"
export GIT_COMMITTER_EMAIL="${GITHUB_USER}@users.noreply.github.com"
export GIT_AUTHOR_NAME='ecwpz91'
export GIT_AUTHOR_EMAIL="${GIT_AUTHOR_NAME}@users.noreply.github.com"

