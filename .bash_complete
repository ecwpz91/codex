# .bash_complete

# ----------------------------------------------------------------------
# AWS CLI Client
# ----------------------------------------------------------------------
if [ -f "$HOME/.local/bin/aws_completer" ]; then
 source "$HOME/.local/bin/aws_completer"
fi

# ----------------------------------------------------------------------
# FeedHenry CLI Client
# ----------------------------------------------------------------------
if [ -f "$HOME/.local/bin/fhc.sh" ]; then
 source "$HOME/.local/bin/fhc.sh"
fi

# ----------------------------------------------------------------------
# OpenShift CLI Client
# ----------------------------------------------------------------------
if [ -f "$HOME/.local/bin/oc.sh" ]; then
 source "$HOME/.local/bin/oc.sh"
fi

# ----------------------------------------------------------------------
# OpenShift CLI Wrapper Script
# ----------------------------------------------------------------------
if [ -f "$HOME/.local/bin/oc-cluster.sh" ]; then
 source "$HOME/.local/bin/oc-cluster.sh"
fi

# ----------------------------------------------------------------------
# OpenShift S2I Client
# ----------------------------------------------------------------------
if [ -f "$HOME/.local/bin/s2i.sh" ]; then
 source "$HOME/.local/bin/s2i.sh"
fi

# ----------------------------------------------------------------------
# Minikube
# ----------------------------------------------------------------------
if [ -f "$HOME/.local/bin/minikube.sh" ]; then
 source "$HOME/.local/bin/minikube.sh"
fi
